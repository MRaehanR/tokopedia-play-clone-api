{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": "error",
        "no-console":"error",
        "no-duplicate-imports": "error",
        "no-template-curly-in-string": "error",
        "no-var": "error",
        "prefer-const": "error",
        "comma-dangle": "error",
        "comma-spacing": "error",
        "indent": ["error", 2]
    }
}
